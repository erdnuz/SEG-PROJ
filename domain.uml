//User classes
class User {
  int userId;
  String firstName;
  String lastName;
  String email;
  String role;
  String enabled;
}

class Admin {
  isA User;
}

class Renter {
  isA User;
  1 -- * Request requests;
}

class Lessor {
  isA User;
  1 -- * Listing listings;
}

// Listing class
class Listing {
  int listingId;
  String description;
  double price;
  1 -- 1 Category category;
  1 -- * Request requests;
  1 -- * Availability availability;
}

class Category {
  String name;
  String description;
}

// Association classes
class Request {
  int requestId;
  String message;
  String status;
}
class Availability {
  1 -- * Slot slots;
}

class Slot {
  long start;
  long end;
}




